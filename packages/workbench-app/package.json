    "@phema/connection-manager": "^1.0.0",
    "@phema/cql-editor": "^1.0.0",
    "@phema/terminology-manager": "^1.0.0",
    "@phema/workbench-common": "^1.0.0",
    "@phema/phenotype-visualizer": "^1.0.0",
    "core-js": "^3.6.5",
    "es6-shim": "^0.35.4",
    "graphlib-dot": "^0.6.4",
    "jsoneditor": "^7.1.0",
    "jsoneditor-react": "^2.0.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.13",
    "moment": "^2.23.0",
    "numeral": "^2.0.6",
