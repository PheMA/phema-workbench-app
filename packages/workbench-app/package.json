{
  "name": "@phema/workbench-app",
  "version": "0.1.0",
  "description": "PhEMA Workbench App",
  "scripts": {
    "start": "yarn run build:worker && parcel serve --port 8989 --log-level verbose --no-cache ./index.html",
    "watch": "parcel --port 8989 watch index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build:worker && parcel build --no-scope-hoist index.html",
    "build:worker": "parcel build --no-scope-hoist ../../node_modules/monaco-editor/esm/vs/editor/editor.worker.js --no-source-maps"
  },
  "author": "PhEMA Collaborators",
  "license": "Apache-2.0",
